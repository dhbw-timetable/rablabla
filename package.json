{
  "name": "Rablabla",
  "version": "1.0.0",
  "description": "An implementation of a web client providing DHBW rapla information.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhbw-timetable/rablabla.git"
  },
  "scripts": {
    "start": "gulp",
    "minify": "browserify ./src/main/webapp/app.js -g [ envify --NODE_ENV production ] -g uglifyify | uglifyjs --compress --mangle > ./src/main/webapp/app-production.js",
    "deploy": "clear\ngulp build\nnpm run minify\nrm -f src/main/webapp/app.js\nmv src/main/webapp/app-production.js src/main/webapp/app.js\nmvn clean install\ncp target/Rablabla.war defaultServer/apps/\nbx cf push Rablabla -p defaultServer"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "envify": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "require-dir": "^0.3.1",
    "uglify-js": "^3.1.6",
    "uglifyify": "^4.0.4",
    "underscore": "^1.8.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browser-sync": "^2.18.13",
    "classnames": "^2.2.5",
    "date-fns": "^1.29.0",
    "dateformat": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "jquery": "^3.2.1",
    "jss": "^8.1.0",
    "material-ui": "^1.0.0-beta.19",
    "material-ui-icons": "^1.0.0-beta.15",
    "pretty-hrtime": "^1.0.3",
    "prop-types": "^15.6.0",
    "react": "15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "15.6.2",
    "react-infinite-calendar": "^2.3.1",
    "react-modal": "^3.1.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-tiny-virtual-list": "^2.1.4",
    "recompose": "^0.26.0"
  }
}
